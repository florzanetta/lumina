{
	"manifest_version": 2,
	
	"name": "Lumina",
	"description": "Extension to do asynchronous stuff against Lumina site.",
	"version": "0.1",
	
	"permissions": [
		// "notifications",
		"alarms",
		"http://localhost:8000/",
		"http://127.0.0.1:8000/"
	],
	
	"background": {
		"scripts": ["eventPage.js"],
		"persistent": false
	},
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	  
	//"web_accessible_resources": [
	//	"icon.png"
	//]
}
