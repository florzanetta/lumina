{
	"manifest_version": 2,
	
	"name": "Lumina",
	"description": "Extension to do asynchronous stuff against Lumina site.",
	"version": "0.1",
	
	"permissions": [
		// "notifications",
		"alarms",
		"storage",
	    "fileSystem",
		"http://localhost:8000/"
	],
	
	"background": {
		"scripts": ["eventPage.js"],
